model  basicFeatureModel
elements: 
	boolean chess
	boolean minorPieces
	boolean board
	boolean largerPieces
	boolean pawns
	boolean pieces
	boolean leaf
	boolean clock
relations:
	r0: coreElements(chess)
	r1:decomposition(chess,[board])<1>
	r2:decomposition(chess,[pieces])<1>
	r3:decomposition(chess,[clock])<0>
	r4:decomposition(pieces,[largerPieces])<0>
	r5:decomposition(pieces,[minorPieces])<0>
	r6:decomposition(pieces,[pawns])<0>
	r7:decomposition(pieces,[leaf])<1>

